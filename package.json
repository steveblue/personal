{
  "name": "personal-site",
  "version": "1.0.0",
  "description": "Stephen Belovarich's Personal Site",
  "repository": "git@github.com:readymade-ui/starter.git",
  "author": "Steve Belovarich <steveblue@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "index.html",
  "targets": {
    "main": false,
    "node": {
      "context": "node",
      "outputFormat": "commonjs",
      "source": "src/server/index.ts",
      "distDir": "dist/server"
    },
    "dev": {
      "source": "src/client/index.html",
      "distDir": "dist/client",
      "publicUrl": "/"
    }
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development vite-node ./src/server/index.ts",
    "start:client": "cross-env NODE_ENV=development vite dev ./src/client --port 4443",
    "preview": "cross-env NODE_ENV=production node dist/server/index.js",
    "serve": "cross-env NODE_ENV=production node dist/server/index.js",
    "db:init": "NODE_ENV=production node dist/db && mv db.json dist/db.json && mv analytic.json dist/analytic.json",
    "clean:dist": "rimraf dist",
    "build": "cross-env NODE_ENV=production yarn clean:dist && yarn build:prod && yarn build:db && yarn db:init",
    "build:prod": "concurrently \"yarn build:index\" \"yarn build:routes\" \"yarn build:server\" && yarn prod:asset",
    "build:index": "vite build ./src/client/ --emptyOutDir false --outDir ../../dist/client --config vite.config.index.js",
    "build:routes": "vite build ./src/client/ --emptyOutDir false --outDir ../../dist/client --config vite.config.routes.js",
    "build:db": "vite build --emptyOutDir false --outDir dist/ --ssr src/server/db.ts --config vite.config.db.js",
    "build:server": "vite build --outDir dist/server --ssr src/server/index.ts --config vite.config.server.js",
    "print:asset": "ls -l dist/client/assets/img",
    "prod:asset": "cp src/client/sitemap.xml dist/client/sitemap.xml && cp src/client/robots.txt dist/client/robots.txt",
    "pretty:check": "prettier \"src/**/*.{js,ts,html,css,scss}\" --check",
    "pretty": "prettier \"src/**/*.{js,ts,html,css,scss}\" --write",
    "lint": "eslint src/**/**/*.ts --fix",
    "test": "echo \"No tests specified\""
  },
  "dependencies": {
    "@fontsource/lato": "^4.5.8",
    "@fontsource/tilt-warp": "^5.1.1",
    "@lit-labs/ssr": "^3.2.2",
    "@readymade/core": "~3.1.3",
    "@readymade/dom": "~3.1.3",
    "@readymade/router": "~3.1.3",
    "@readymade/ui": "~3.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@skatejs/ssr": "^0.19.11",
    "@types/node-fetch": "^2.5.5",
    "@ungap/custom-elements": "^1.3.0",
    "body-parser": "^1.19.0",
    "broadcastchannel-polyfill": "^1.0.1",
    "browserslist": "^4.14.0",
    "caniuse-lite": "^1.0.30001119",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "compression": "^1.7.4",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "html-minifier-terser": "^7.2.0",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsdom": "^25.0.1",
    "lowdb": "~5.0.0",
    "node-fetch": "2.6.7",
    "node-window-polyfill": "^1.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "vite-node": "^2.1.2",
    "web-animations-js": "^2.3.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@eslint/js": "^9.12.0",
    "@originjs/vite-plugin-global-style": "^1.0.2",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@rollup/plugin-url": "^8.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/he": "^1",
    "@types/node": "^22.7.4",
    "@types/serve-static": "^1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "advanced-sitemap-generator": "^1.7.0",
    "autoprefixer": "^9.7.4",
    "chokidar": "^4.0.1",
    "chromedriver": "^129.0.2",
    "copyfiles": "^2.4.1",
    "cssnano": "^7.0.6",
    "esbuild": "^0.25.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "globals": "^15.10.0",
    "he": "^1.2.0",
    "helmet": "^8.0.0",
    "htmlnano": "^2.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lit": "^3.2.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.3.3",
    "rollup": "4.24.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-inline-postcss": "^3.0.1",
    "rollup-plugin-module-replacement": "^1.2.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-string-html": "^1.0.0",
    "serve-static": "^1.16.2",
    "stream-browserify": "3",
    "tachometer": "^0.7.1",
    "tslib": "~2.7.0",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-node": "^2.1.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-standard-css-modules": "^0.0.11",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "externals": [
    "./src/server/config.js"
  ],
  "alias": {
    "config": "./src/config.ts"
  },
  "packageManager": "yarn@4.5.0"
}
